{
	"expo": {
		"scheme": "echogame",
		"name": "Echo",
		"slug": "echogame",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#2A2A2A"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"intentFilters": [
				{
					"autoVerify": true,
					"data": {
						"scheme": "https",
						"host": "new.echogame.xyz",
						"pathPrefix": "/"
					},
					"action": "VIEW",
					"category": ["BROWSABLE", "DEFAULT"]
				}
			],
			"package": "dk.novusgroup.echo",
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#2A2A2A"
			}
		},
		"web": {
			"bundler": "metro"
		},
		"plugins": [
			"expo-secure-store",
			"expo-router",
			[
				"expo-font",
				{
					"fonts": [
						"node_modules/@expo-google-fonts/inter/Inter_900Black.ttf",
						"node_modules/@expo-google-fonts/space-mono/SpaceMono_400Regular.ttf"
					]
				}
			]
		],
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "81cbea00-b05f-48a1-a52c-6882c13899fa"
			}
		}
	}
}
